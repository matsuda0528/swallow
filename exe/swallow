#! /usr/bin/env ruby

$LOAD_PATH.unshift File.expand_path("../lib", __dir__)
require "swallow"
require "pry"

module Swallow
  # AUK Parser
  parser = AUKParser.new
  parser.parse File.read(ARGV[0]) if ARGV[0]
  ast = parser.ast

  # SAT Encoder
  binding.pry
  # puts ast.to_html

  # AUK Generator
  # auk = parser.ast.to_auk
  # puts auk
end
